.ui-charts-tooltip table {width:95%;white-space:nowrap}
.ui-charts-tooltip th {text-align:left;white-space:nowrap}
.ui-charts-tooltip td {padding-left:10px;white-space:nowrap;text-align:right}
.ui-charts-tooltip p, .ui-charts-tooltip h3 {padding:0px;margin:0px;font-size:12px}
.ui-charts-tooltip h3 {border-bottom: 1px solid #50BFC6;color:#282828;padding-bottom:5px;margin-bottom:5px}
